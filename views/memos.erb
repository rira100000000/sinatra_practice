<div class=mid_box>
  <a href='http://localhost:4567/memos/new' class=button>新規作成</a><br>
  <% unless @files.nil? %>
    <% @files.each do |file| %>
      <% id, title, delete_flag = file.split(',') %>
      <% unless delete_flag == "true\n" %>
        <a href="<%= "http://localhost:4567/memos/#{id}/show" %>"> <%= "●" + title %> </a><br>
      <% end %>
    <% end %>
  <% end %>
</div>
